void drawPlayer(float x, float y, float s)
{
  pushMatrix();
    translate(x,y);
    scale(s);
    
    fill(0,255,0);
    stroke(0,255,0);
    rect(0,4,1,4);
    rect(1,3,13,5);
    rect(6,1,3,2);
    rect(7,0,1,1);
    rect(14,4,1,4);
  popMatrix();
}

